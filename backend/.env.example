# Example environment configuration for ThreatDock backend

# GitHub token (optional, improves GitHub Advisories API rate limiting)
GITHUB_TOKEN=

# NVD API key (optional, improves NVD API rate limiting)
NVD_API_KEY=

# OTX API key (optional, required to fetch subscribed pulses)
OTX_API_KEY=

# ThreatFox Auth Key (optional, required to fetch recent IOCs)
THREATFOX_AUTH_KEY=

# MISP settings (optional, if integrating with a MISP instance)
MISP_URL=
MISP_API_KEY=

# IntelOwl API key (optional, reserved for future expansion)
INTELO_OWL_API_KEY=